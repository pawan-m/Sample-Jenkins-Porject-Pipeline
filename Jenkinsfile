pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Clone') {
            steps {
                echo 'Cloning the code'
            }
        }
        stage('Build') {
            steps {
                echo 'Building the code'
            }
        }
        stage('Package') {
            steps {
                echo 'Package the code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Package the code'
            }
        }
    }
}
